{
  "name": "zenbeasts-root",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "ts-mocha -p ./tsconfig.json -t 1000000 tests/**/*.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --check '**/*.{ts,json,md}'",
    "format:fix": "prettier --write '**/*.{ts,json,md}'",
    "build": "tsc -p tsconfig.json",
    "build:program": "anchor build",
    "build:frontend": "cd frontend && npm run build",
    "deploy:devnet": "node scripts/build-and-deploy.js devnet",
    "deploy:mainnet": "node scripts/build-and-deploy.js mainnet",
    "deploy:frontend:vercel": "node scripts/deploy-frontend.js vercel production",
    "deploy:frontend:netlify": "node scripts/deploy-frontend.js netlify production",
    "deploy:frontend:preview": "node scripts/deploy-frontend.js vercel preview",
    "verify:deployment": "npm run build && node dist/scripts/verify-deployment.js",
    "initialize": "node dist/scripts/initialize.js",
    "mint-sample": "node dist/scripts/mint-sample.js",
    "test:activity": "node dist/scripts/perform-activity.js",
    "test:upgrade": "node dist/scripts/upgrade-test.js",
    "test:breed": "node dist/scripts/breed-test.js"
  },
  "devDependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@eslint/js": "^9.39.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.0",
    "@solana/spl-token": "^0.4.7",
    "@solana/web3.js": "^1.95.6",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  }
}
